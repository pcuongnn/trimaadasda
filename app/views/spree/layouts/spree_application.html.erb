<!DOCTYPE html>
<html lang="ru_RU"> 
<head>
	<%= render partial: 'spree/shared/head' %>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
	<%= stylesheet_link_tag  'spree/frontend/bootstrap.min.css', media: 'all', 'data-turbolinks-track' => true %>
	<%= stylesheet_link_tag  'spree/frontend/style.min.css', media: 'all', 'data-turbolinks-track' => true %>
	<script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
	<%= javascript_include_tag 'spree/frontend/form.js'%>

	
	<!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
	<!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
	<!--[if lt IE 9]>
		<script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
		<script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
	<![endif]-->
</head>
<body >
	<div class="sticky-page">
		<section class="header">
			<div class="header-lvl-1">
				<div class="container">
					<div class="row">
						<div class="col-xs-12 text-right">
							<ul class="nav-login">
								<li><a href="/admin">DISTRIBUTOR LOGIN</a></li>
								<% if spree_current_user %>
									<li><%= link_to 'Log Out', destroy_spree_user_session_path %></li>	
									<li><%= link_to 'My Account', spree.account_path %></li>
								<% else %>
									<li><a href="/login">Log In</a></li>
									<li><a href="/signup">Register</a></li>
								<% end %>
							</ul>
							<ul class="nav-social">
								<li><a href="#" class="icon fb"><%= image_tag "spree/frontend/social-f.png"%></a></li>
								<li><a href="#" class="icon tw"><%= image_tag "spree/frontend/social-i.png"%></a></li>
								<li><a href="#" class="icon in"><%= image_tag "spree/frontend/social-t.png"%></a></li>
							</ul>
						</div>
					</div>
				</div>
			</div>
			<div class="header-lvl-2">
				<div class="container">
					<div class="row">
						<div class="col-xs-12">
							<div class="my-flex">
								<div class="logo"><%= image_tag "spree/frontend/logo.png"%></div>
								<div class="form">
									<form action="#">
										<input type="text" class="search" placeholder="Type to Search">
									</form>
									<div><a href="#" class="btn btn-primary grey"><span><%= image_tag "spree/frontend/head-search.png"%></span>  <span>SEARCH</span></a></div>
									<div><a href="/cart" class="btn btn-primary white-blue"><span><%= image_tag "spree/frontend/head-card.png"%></span> <span>MY CART</span> <span class="count"></span></a></div>
									<div><a href="/checkout" class="btn btn-primary dark-grey"> <span>CHECK OUT</span></a></div>
								</div>
							</div>
						</div>
					</div>
					<div class="row">
						<div class="col-xs-12">
							<ul class="nav-menu">
								<li><a href="/">home</a></li>
								<% Spree::Taxon.where(parent_id: nil).each do |tax| %>
									<li><%= link_to tax.name, show_taxonomies_path(tax.id) %></li>
								<% end %>
								<li><a href="#">New Products</a></li>
								<li><a href="/contact-us">Contact Us</a></li>
							</ul>
						</div>
					</div>
				</div>
			</div>
		</section>
		<section class="content">
			<div class="container">
				<%= yield %>
			</div>
		</section>
	<section class="footer">
		<div class="footer-lvl-1">
			<div class="container">
				<div class="row">
					<div class="col-xs-12 text-right">
						<div class="border">
							<ul class="nav-menu">
								<li><a href="/">Home</a></li>
								<li><a href="/about-us">About Us</a></li>
								<li><a href="/contact-us">Contact Us</a></li>
								<li><a href="#">LITERATURE</a></li>
								<li><a href="#">How to Order</a></li>
								<li><a href="#">Feedback/Suggestions</a></li>
								<li><a href="#">Disclaimer</a></li>
								<li><a href="#">Return Policy</a></li>
							</ul>
						</div>
					</div>
				</div>
			</div>
		</div>
		<div class="footer-lvl-2">
			<div class="container">
				<div class="row">
					<div class="col-xs-12 text-right">
						<div class="my-flex">
							<div>
								<p class="text-uppercase copy">&copy; 2015 COPYRIGHT TRIMTOTHETRADE.cOM</p>
							</div>
							<div>
								<ul class="nav-social">
									<li><a href="#" class="icon fb"><%= image_tag "spree/frontend/social-f.png"%></a></li>
									<li><a href="#" class="icon tw"><%= image_tag "spree/frontend/social-i.png"%></a></li>
									<li><a href="#" class="icon in"><%= image_tag "spree/frontend/social-t.png"%></a></li>
								</ul>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>

	</section>
</body>
	<%= javascript_include_tag 'spree/frontend/owl.carousel.min.js'%>
	<!--<link href='assets/css/fontello.css' rel='stylesheet' type='text/css'>-->
	<%= stylesheet_link_tag  'spree/frontend/owl.carousel.css', media: 'all', 'data-turbolinks-track' => true %>
	
	<%= javascript_include_tag 'spree/frontend/validator.min.js'%>
	<%= javascript_include_tag 'spree/frontend/SmoothScroll.js'%>
	<!--<script type="text/javascript" src="assets/js/typeahead.js"></script>-->
	<%= javascript_include_tag 'spree/frontend/main.js'%>
</html>